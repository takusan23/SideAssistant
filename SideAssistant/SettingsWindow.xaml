<Window x:Class="SideAssistant.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SideAssistant"
        mc:Ignorable="d"
        Title="SettingsWindow" Height="450" Width="800">
    <DockPanel>
        <DockPanel Margin="10">
            <TextBlock DockPanel.Dock="Top" FontSize="20" Text="取得するRSSのリンク設定"></TextBlock>
            <TextBox DockPanel.Dock="Top"  Text="" x:Name="SettingRSSLink"></TextBox>
            <Button Click="Button_Click" DockPanel.Dock="Top" Content="保存" HorizontalAlignment="Right" Margin="10" FontSize="15"></Button>
            <StackPanel></StackPanel>
        </DockPanel>
    </DockPanel>
</Window>
